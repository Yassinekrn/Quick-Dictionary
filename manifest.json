{
    "manifest_version": 3,
    "name": "Quick Dictionary",
    "version": "1.0",
    "permissions": ["activeTab"],
    "host_permissions": ["*://api.dictionaryapi.dev/*"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}
